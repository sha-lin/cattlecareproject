    {
      "version": 2,
      "builds": [
        {
          "src": "vetfarm/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": { "distDir": "staticfiles" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "your_project_name/wsgi.py"
        },
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        }
      ]
    }
